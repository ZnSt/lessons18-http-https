  background-color: #00796b;\r\n  cursor: pointer;\r\n  box-shadow: 0 4px 4px 0 rgba(#414141, 0.3);\r\n  &:hover {\r\n    background-color: #004c40;\r\n  }\r\n  &:active {\r\n    box-shadow: 0 6px 4px 0 rgba(#414141, 0.3);\r\n  }\r\n}\r\n\r\n.search-form {\r\n  display: flex;\r\n  justify-content: start;\r\n  padding: 20px;\r\n  box-shadow: 0 0 2px 2px rgba(#414141, 0.2);\r\n}\r\n\r\n.search-form input {\r\n  margin-right: 12px;\r\n  padding: 4px;\r\n  outline: 0;\r\n  border: 0;\r\n  color: #263238;\r\n  font-size: 18px;\r\n  background-color: transparent;\r\n  border-bottom: 2px solid #00796b;\r\n  &::placeholder {\r\n    color: #616161;\r\n  }\r\n}\r\n\r\n.result-1,\r\n.result-3,\r\n.result-4 {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: flex-start;\r\n}\r\n\r\ntable.minimalistBlack {\r\n  margin-top: 20px;\r\n  border: 3px solid #000000;\r\n  text-align: left;\r\n  border-collapse: collapse;\r\n}\r\n\r\ntable.minimalistBlack td,\r\ntable.minimalistBlack th {\r\n  border: 1px solid #000000;\r\n  padding: 5px 10px;\r\n}\r\n\r\ntable.minimalistBlack thead {\r\n  background: #cfcfcf;\r\n  background: linear-gradient(to bottom, #dbdbdb 0%, #d3d3d3 66%, #cfcfcf 100%);\r\n  border-bottom: 3px solid #000000;\r\n}\r\n\r\ntable.minimalistBlack thead th {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n  color: #000000;\r\n  text-align: left;\r\n}\r\n\r\n.error {\r\n  color: red;\r\n}\r\n\r\n/* products */\r\n\r\n.product-list {\r\n  margin: 0;\r\n  padding: 0;\r\n  list-style: none;\r\n}\r\n\r\n.product-list li {\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 5px;\r\n}\r\n\r\n.product-list li .content {\r\n  display: flex;\r\n}\r\n\r\n/* weather */\r\n\r\n.weather div {\r\n  margin-left: 40px;\r\n}\r\n\r\n/* movies */\r\n\r\nheader {\r\n  display: flex;\r\n  padding-bottom: 30px;\r\n}\r\n\r\n.movies {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n\r\n.movies div.movie-card {\r\n  width: 240px;\r\n  padding: 10px;\r\n}\r\n\r\nbutton,\r\n[data-load-more] {\r\n  display: block;\r\n}\r\n\r\nbutton.is-hidden {\r\n  display: none;\r\n}\r\n\r\nbutton.btn-service > .spinner-grow.is-hidden {\r\n  display: none;\r\n}\r\n"},"lineCount":192}},"error":null,"hash":"251ec3ea963d1157683a70eca2e7916b","cacheData":{"env":{}}}